<!--pages/mine/mine.wxml-->
<view class="avatar">
  <image class="userinfo-avatar" src="{{userInfo.avatar_url}}"></image>
  <text class="userinfo-nickname">{{userInfo.username}}</text>
</view>
 <view class="mune_interface">
    <view class="ispublish_style" bindtap="information" >
      <view>
        <image src="../img/xinxi.png" class="menu_image"></image>
        <text class="word">我的信息</text>
      </view>
    </view>
    <view class="line01"></view>
    <view class="waspublish_style" bindtap="collect">
      <view>
        <image src="../img/minesc.png" class="menu_image"></image>
        <text class="word">我的收藏</text>
      </view>
    </view>
  </view>

  <view class="line03"></view>
  <view class="mune_interface">
    <view class="ispublish_style" bindtap="fabu">
      <view>
        <image src="../img/fabu_c.png" class="menu_image"></image>
        <text class="word">我的发布</text>
      </view>
    </view>
    <view class="line01"></view>
    <view class="waspublish_style" bindtap="order">
      <view>
        <image src="../img/dingdan.png" class="menu_image"></image>
        <text class="word">我的订单</text>
      </view>
    </view>
  </view>
  <view class="line03"></view>
