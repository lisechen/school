/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.center{
  display: flex;
  justify-content: center;
  align-content: center
}
.arrow{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.arrow::after{
  content: "";
  width: 15rpx;
  height: 15rpx;
  border-top: 3rpx solid #000;
  border-right: 3rpx solid #000;
  transform: rotate(45deg);
}
page,view,text,swiper,swiper-item,navigator{
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}
page{
  --themeColor: 	#CD96CD;
}